@page
@model SafetyComeback.App.FrontEnd.Pages.FSede.EditSedeModel
@{
    ViewData["Title"] = "Editar Sede";
}
<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <h3>Editar Sede</h3>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-10">
            <form method="post" asp-action="Edit">
                <input type="hidden" asp-for=@Model.Sede.Id />
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label class="control-label" asp-for=@Model.Sede.Nombre></label>
                    <input class="form-control" asp-for=@Model.Sede.Nombre />
                    <span asp-validation-for=@Model.Sede.Nombre class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input class="btn btn-primary" value="Editar Sede" type="submit" />
                </div>
            </form>
        </div>
    </div>

</div>