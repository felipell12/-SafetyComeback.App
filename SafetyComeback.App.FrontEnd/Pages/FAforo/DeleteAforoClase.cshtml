@page
@model SafetyComeback.App.FrontEnd.Pages.FAforo.DeleteAforoClaseModel
@{
    ViewData["Title"] = "EliminarAsistencia";
}
<div class="container">
    <div class="row">
        <div class="col-sm-12">
            <h3>Eliminar Registro de Asistencia</h3>
        </div>
    </div>
    <div class="row">
        <div class="col-sm-10">
            <form method="post">
                <input type="hidden" asp-for=@Model.AforoClase.Id />
                <div class="form-group">
                    <label class="control-label" asp-for=@Model.AforoClase.PersonaId></label>
                    <input class="form-control" asp-for=@Model.AforoClase.PersonaId disabled/>
                    </div>
                <div class="form-group">
                    <input class="btn btn-primary" value="Eliminar Registro" type="submit" />
                </div>
            </form>
        </div>
    </div>

</div>